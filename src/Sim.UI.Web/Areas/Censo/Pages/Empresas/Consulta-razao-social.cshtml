@page
@model Sim.UI.Web.Areas.Censo.Pages.Empresas.Consulta_razao_socialModel
@{
    Layout = "_Layout.cshtml";
    ViewData["Title"] = "Lista";
    ViewData["ActivePage"] = EmpresaNavPages.ConsultaNome;
}
<!--<partial name="_StatusMessage" model="Model.StatusMessage" />-->

<form class="section" method="post">
    <div class="row">
        <div class="col s12">
            <div class="col s12 m4 l4">
                <div class="input-field">
                    <input asp-for="Input.RazaoSocial" />
                    <label asp-for="Input.RazaoSocial"></label>
                </div>
            </div>
            <div class="col s12 m2 l2">
                <div class="input-field">
                    <button class="btn waves-effect" type="submit" role="button"><i class="fa fa-search"></i></button>
                </div>
            </div>
        </div>
    </div>
</form>

<div class="card">

    <partial name="_EmpresaList" model="Model.Input.ListaEmpresas" />

</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
    <script src="~/js/jquery.mask.min.js"></script>
    <script>
        $('#inputCNPJ').mask('00.000.000/0000-00', { placeholder: "__.___.___/____-__" });
    </script>
}