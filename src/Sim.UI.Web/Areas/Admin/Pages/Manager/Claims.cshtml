@page
@model Sim.UI.Web.Areas.Admin.Pages.Manager.ClaimsModel
@{
    Layout = "_Layout.cshtml";

    ViewData["Title"] = "Claims";
}


<partial name="_StatusMessage" model="Model.StatusMessage" />


<div>
    <form method="post">
        <div class="row">
            <div class="col s12">
                <div class="col s4 m4 l4">
                    <div class="input-field">
                        <input asp-for="Input.ClaimType" />
                        <label asp-for="Input.ClaimType"></label>
                    </div>
                </div>
                <div class="col">
                    <div class="input-field">
                        <button class="btn waves-effect" type="submit">Adicionar</button>
                    </div>
                </div>
            </div>
        </div>
    </form>
</div>