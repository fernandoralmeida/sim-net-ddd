@page
@model Sim.UI.Web.Areas.Admin.Pages.Manager.ClaimsModel
@{
    Layout = "_Layout.cshtml";

    ViewData["Title"] = "Claims";
    ViewData["ActivePage"] = AdminNavPages.Claims;
}


<partial name="_StatusMessage" model="Model.StatusMessage" />


<div >
    <div class="col-md-5">
        <form asp-action="Claims" method="post">
            <div class="row g-3">
                <div class="d-flex mb-3">
                    <input asp-for="Input.ClaimType" class="form-control me-2" disabled />
                    <button class="btn btn-outline-primary" type="submit" disabled>Adicionar</button>
                </div>
            </div>
        </form>
    </div>
    <hr />
</div>