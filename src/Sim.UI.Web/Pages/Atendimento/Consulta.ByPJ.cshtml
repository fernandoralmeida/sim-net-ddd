@page
@model Sim.UI.Web.Pages.Atendimento.ConsultaModel
@{
    Layout = "_Layout.cshtml";
    ViewData["Title"] = "Consultas por PJ";
    ViewData["ActivePage"] = AtendimentoNavPages.Consulta;
}


<partial name="_StatusMessage" model="Model.StatusMessage" />


<form method="post" asp-page-handler="ListByEmpresa">
    <div class="row">
        <div class="col s10 m4 l4 input-field">
            <input asp-for="Input.CNPJ" />
            <label>CNPJ</label>
        </div>
        <div class="col input-field">
            <button asp-page-handler="ListByEmpresa" class="btn waves-effect" type="submit" role="button"><i class="material-icons">search</i></button>
        </div>
    </div>
</form>

<partial name="_AtendimentoList" model="Model.Input.ListaAtendimento" />