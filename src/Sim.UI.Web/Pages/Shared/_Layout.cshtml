
<!DOCTYPE html>
<html lang="pt-br">
<head>
    <meta charset="utf-8">
    <link rel="shortcut icon" href="~/favicon.ico" />
    <!--Import Google Icon Font-->
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <!--Import materialize.css-->
    <link rel="stylesheet" href="~/lib/materialize/css/materialize.min.css" />
    <link rel="stylesheet" href="~/css/site.css">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.7.0/css/all.css" integrity="sha384-lZN37f5QGtY3VHgisS14W3ExzMWZxybE1SJSEsQp9S+oqd12jhcu+A56Ebc1zFSJ" crossorigin="anonymous">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>SIM - @ViewData["Title"]</title>
</head>
<body class="has-fixed-sidenav">
    <header>

        <ul id="sidenav-left" class="sidenav sidenav-fixed">
            <li>
                <p></p>
                <a asp-area="" asp-page="/Index" class="logo-container">SEDEMP<i class="material-icons">widgets</i></a>
                <p></p>
            </li>
            <li class="no-padding">
                <ul class="collapsible collapsible-accordion transparent">
                    <li class="divider"></li>
                    <li class="waves-block waves-effect">
                        <a class="collapsible-header">Inicio</a>
                        <div class="collapsible-body transparent">
                            <ul>
                                <li><a href="#" class="waves-effect active">PAT<i class="fas fa-industry"></i></a></li>
                                <li><a href="#" class="waves-effect"><i class="fas fa-hand-holding-usd"></i>Banco do Povo</a></li>
                                <li><a class="waves-effect" asp-area="" asp-page="/SebraeAqui/Index"><i class="fas fa-handshake"></i>Sebrae Aqui</a></li>
                                <li><a href="#" class="waves-effect"><i class="fas fa-building"></i>Sala do Empreendedor</a></li>
                            </ul>
                        </div>
                    </li>
                    <li class="waves-block waves-effect">
                        <a class="collapsible-header">Atendimentos</a>
                        <div class="collapsible-body transparent">
                            <ul>
                                <li><a asp-page="/Atendimento/Index" class="waves-effect">Meus Atendimentos<i class="material-icons">access_time</i></a></li>
                                <li><a asp-page="/Atendimento/Novo" class="waves-effect">Atendimento Ativo<i class="material-icons">access_alarm</i></a></li>
                                <li><a asp-page="/Atendimento/Consulta" class="waves-effect">Consultar<i class="material-icons">search</i></a></li>
                            </ul>
                        </div>
                    </li>
                    <li class="waves-block waves-effect">
                        <a class="collapsible-header">Clientes</a>
                        <div class="collapsible-body transparent">
                            <ul>
                                <li><a asp-page="/Pessoa/Index" class="waves-effect">Pessoas<i class="material-icons">group</i></a></li>
                                <li><a asp-page="/Empresa/Index" class="waves-effect">Empresas<i class="material-icons">domain</i></a></li>
                            </ul>
                        </div>
                    </li>
                    <li class="waves-block waves-effect">
                        <a class="collapsible-header">Agenda</a>
                        <div class="collapsible-body transparent">
                            <ul>
                                <li><a asp-page="/Agenda/Index" class="waves-effect">Eventos<i class="material-icons">event</i></a></li>
                                <li><a asp-page="/Planner/Index" class="waves-effect">e-Planner<i class="material-icons">description</i></a></li>
                            </ul>
                        </div>
                    </li>
                    <li class="waves-block waves-effect">
                        <a class="collapsible-header">Power BI</a>
                        <div class="collapsible-body transparent">
                            <ul>
                                <li><a href="#" class="waves-effect">Atendimentos<i class="material-icons">access_time</i></a></li>
                                <li><a asp-area="Censo" asp-page="/Empresas/Index" class="waves-effect">Empresas<i class="material-icons">domain</i></a></li>
                                <li><a href="#" class="waves-effect">Pessoas<i class="material-icons">people</i></a></li>

                            </ul>
                        </div>
                    </li>

                    @if (User.IsInRole("Administrador"))
                    {
                        <li class="waves-block waves-effect">
                            <a class="collapsible-header">Configurações</a>
                            <div class="collapsible-body transparent">
                                <ul>
                                    <li><a class="waves-effect" asp-area="Settings" asp-page="/Common/Index">Sistema<i class="material-icons">settings</i></a></li>
                                    <li><a asp-area="Admin" asp-page="/Manager/Index" class="waves-effect">Contas<i class="material-icons">account_circle</i></a></li>
                                </ul>
                            </div>
                        </li>

                    }
                    <partial name="_LoginPartial" />
                    <li><div class="divider"></div></li>
                </ul>
            </li>
            <li>
                <div class="center">
                    &copy; @DateTime.Now.Year - Sim.Web
                    <a asp-area="" asp-page="/Privacy">Privacy</a>
                </div>
            </li>
        </ul>
    </header>
    <main>
        <div>
            @RenderBody()
        </div>
    </main>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script type="text/javascript" src="~/lib/materialize/js/materialize.min.js"></script>
    <script type="text/javascript" src="~/lib/masonry/masonry.pkgd.min.js"></script>
    <script type="text/javascript" src="~/js/site.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
