@page
@model Sim.UI.Web.Pages.Empresa.Jucesp_consulta_logradouroModel

@{
    Layout = "_Layout.cshtml";
    ViewData["Title"] = "Lista";
}

<!--<partial name="_StatusMessage" model="Model.StatusMessage" />-->

    <div class="card">
        <form method="post" class="row" style="border-bottom:solid 1px #efefef; margin-bottom:0px;">
            <div class="col m6 l6 s12">
                <nav class="z-depth-0">
                    <div class="nav-wrapper white">
                        <div class="input-field">
                            <input asp-for="Input.Logradouro" placeholder="Logradouro" type="search" required>
                            <label class="label-icon" asp-for="Input.Logradouro"><i class="black-text material-icons">search</i></label>
                        </div>
                    </div>
                </nav>
            </div>
            <div class="col valign-wrapper" style="height:64px;">
                <div class="col">
                    <input asp-for="Input.Municipio" />
                </div>
                <div class="col">
                    <button class="btn waves-effect waves-light" type="submit" role="button">Pesquisar</button>
                </div>
            </div>
        </form>
        <partial name="_EmpresaList" model="Model.Input.ListaEmpresas" />
    </div>
