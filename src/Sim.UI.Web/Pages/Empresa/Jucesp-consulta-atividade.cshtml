@page
@model Sim.UI.Web.Pages.Empresa.Jucesp_consulta_atividadeModel

@{
    Layout = "_Layout.cshtml";
    ViewData["Title"] = "Lista";
    ViewData["ActivePage"] = EmpresaNavPages.Jucesp;
}

<!--<partial name="_StatusMessage" model="Model.StatusMessage" />-->

    <form method="post">
        <div class="row">
                <div class="col s12 m4 l4">
                    <div class="input-field">
                        <input asp-for="Input.Municipio" id="inputmn" />
                        <label asp-for="Input.Municipio" for="inputmn"></label>
                    </div>
                </div>
                <div class="col s12 m5 l5 valign-wrapper">
